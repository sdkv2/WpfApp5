<Window x:Class="WpfApp1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="MainWindow" Height="146" Width="464">


    <WrapPanel DockPanel.Dock="Top" Margin="11,10,9,10" RenderTransformOrigin="0.5,0.5">
        <WrapPanel.RenderTransform>
            <TransformGroup>
                <ScaleTransform/>
                <SkewTransform AngleX="-0.783"/>
                <RotateTransform Angle="-0.233"/>
                <TranslateTransform X="-1.736"/>
            </TransformGroup>
        </WrapPanel.RenderTransform>
        <Button x:Name="btnOpenFile" Click="btnOpenFile_Click" Width="120" Content="Process CSV FILE" RenderTransformOrigin="0.474,2.512" Height="46"/>
        <ProgressBar x:Name="progressbar" HorizontalAlignment="Center" Height="35" Margin="0,0,0,0" VerticalAlignment="Top" Width="284" ValueChanged="progressbar_ValueChanged_1"/>
    </WrapPanel>
</Window>
